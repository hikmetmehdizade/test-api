{
  "name": "jest-test-project",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start:prod": "node ./dist/server.js",
    "start:dev": "nodemon --watch \"./**/*.ts\" --exec ts-node server.ts",
    "dev": "npm run prisma:migrate && npm run prisma:generate  && npm run start:dev",
    "prisma:copy": "copyfiles -u 1 ./prisma/schema.prisma ./dist/prisma/generated/",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate --schema ./prisma/schema.prisma",
    "test": "npm run migrate:reset && npm run migrate && jest",
    "test:watch": "npm run test -- --watch",
    "format": "prettier --write \"./**.js\" \"./**.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.6.0",
    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "recursive-readdir": "^2.2.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/recursive-readdir": "^2.2.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "copyfiles": "^2.4.1",
    "jest": "^29.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "prisma": "^4.6.0",
    "rimraf": "^3.0.2",
    "superagent": "^8.0.3",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
