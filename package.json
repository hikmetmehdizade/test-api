  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start:prod": "node ./dist/server.js",
    "start:dev": "ts-node server.ts",
    "dev": "npm run prisma:migrate && npm run prisma:generate  && ts-node server.ts",
